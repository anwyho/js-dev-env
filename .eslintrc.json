{
    "root": true,
    "extends": [
        "eslint:recommended",
        "plugin:import/errors",
        "plugin:import/warnings"
    ],
    "parserOptions": {
        "ecmaVersion": 7,
        "sourceType": "module"
    },
    "env": {
        "browser": true,
        "node": true,
        "mocha": true
    },
    "__comment": "For overriding rules, 0 - Nothing, 1 - Warning, 2 - Error",
    "rules": {
        "no-console": 1
    }
}